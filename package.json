{
  "name": "pactjs-test-demo",
  "version": "0.0.1",
  "description": "Demo of consumer driven contract testing using Pact JS",
  "repository": "https://github.com/jimCresswell/pactjs-test-demo.git",
  "author": "jimCresswell <jim.cresswell@gmail.com>",
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "test:unit": "mocha --config .mocharc.unit.yaml",
    "test:unit:debug": "mocha --config .mocharc.unit.yaml --inspector-brk",
    "test:provider:e2e": "mocha './provider/e2e_test/**/*.spec.js'",
    "test:consumer:e2e": "mocha './consumer/e2e_test/**/*.spec.js'",
    "start": "concurrently \"npm run start:provider\" \"npm run start:consumer\"",
    "start:provider": "nodemon --watch ./provider ./provider/start.js",
    "start:consumer": "nodemon --watch ./consumer -e js,json,hbs ./consumer/start.js"
  },
  "dependencies": {
    "axios": "^0.19.1",
    "concurrently": "^5.0.2",
    "express": "^4.17.1",
    "hbs": "^4.0.6",
    "lodash.clonedeep": "^4.5.0",
    "lodash.remove": "^4.7.0",
    "morgan": "^1.9.1"
  },
  "devDependencies": {
    "axios-mock-adapter": "^1.17.0",
    "chai": "^4.2.0",
    "cheerio": "^1.0.0-rc.3",
    "eslint": "^6.7.2",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.19.1",
    "mocha": "^6.2.2",
    "nodemon": "^2.0.2",
    "supertest": "^4.0.2"
  }
}
